	"find_selected_text": true,
	"fold_buttons": false,
	"font_face": "Ricty Diminished",
	"font_options":
	[
		"subpixel_antialias",
		"gdi"
	],
	"font_size": 11,
	"gutter": true,
	"highlight_line": true,
	"ignored_packages":
	[
		"auto-save",
		"Emmet",
		"Git",
		"GitGutter",
		"TortoiseSVN",
		"Vintage"
	],
	"indent_guide_options":
	[
		"draw_normal",
		"draw_active"
	],
	"line_padding_bottom": -1,
